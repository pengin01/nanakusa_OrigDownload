{
  "manifest_version": 2,
  "name": "ふじPの七草粥",
  "version": "2.0",
  "description": "ツールバーのアイコンを押すとスタート.",
  "icons": {
    "16": "icons/icon_16.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "Tab Viewer"
  },
  "background": {
    "scripts": [
      "lib/jquery-3.2.1.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions":[
    "tabs"
  ]
}
